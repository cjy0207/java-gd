package sec02.exam03;

public class LongExample {

	public static void main(String[] args) {
		//long(8byte)
		
		long var1 = 10;
		long var2 = 20L; //L을 붙이거나 안붙이거나 상관 없
		long var3 = 20000000000; //자바 컴파일러는 정수 리터럴을 기보넞ㄱ으로 int타입으로 해석
		//200억이 int타입의 범위를 너기 떄문에 컴파일 에러 발생
		//컴파일에게 int타입이 아니라 long타입임을 알려줘야
		
		long var4 = 20000000000L;
	}

}
